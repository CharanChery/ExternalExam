pipeline{
  agent any
  stages{
    stage('Validation'){
      steps{
        echo 'Validating your code'
      }
    }
    stage('Compilation'){
      steps{
        echo 'Compiling your code'
      }
    }
     stage('Installation')
      steps{
        echo 'Installing your code'
      }
    }
     stage('Deployment'){
      steps{
        echo 'Deploying the code'
      }
    }
}
}
